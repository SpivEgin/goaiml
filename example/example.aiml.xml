<?xml version="1.0" encoding="UTF-8"?>
<aiml version="1.0.1" encoding="UTF-8">

    <category>
        <pattern>MY NAME IS *</pattern>
        <template>
            Nice to meet you <set name="user"><star/></set>
        </template>
    </category>
    <category>
        <pattern>MY DOGS NAME IS *</pattern>
        <template>
            That is interesting that you have a dog named
            <set name="dog">
                <star />
            </set>
        </template>
    </category>
    <category>
        <pattern>WHAT IS MY DOGS NAME</pattern>
        <template>
            <condition>
                <li name="dog">
                    Your dog's name is
                    <get name="dog" />
                </li>
                <li>
                    I don't know
                </li>
            </condition>
        </template>
    </category>
    <category>
        <pattern>
            DO YOU HAVE ANY IDEA
        </pattern>
        <template>
            <condition>
                <li name="user">No, I'm sorry <get name="user"/>, What you saying ?</li>
                <li>No, I'm sorry, What you saying ?</li>
            </condition>
        </template>
    </category>
    <category>
        <pattern>
            <bot name="name" />
            *
        </pattern>
        <template>What's up ?</template>
    </category>
    <category>
        <pattern>I AM *</pattern>
        <template>
            <random>
                <li>
                    Why are you
                    <star />
                </li>
                <li>Do your friends call you</li>
                <li>
                    My name is
                    <bot name="name" />
                </li>
            </random>
        </template>
    </category>
    <category>
        <pattern>DO YOU THINK</pattern>
        <template>
            <think>
                <set name="goodquestion">goodquestion</set>
            </think>
            Maybe :D
        </template>
    </category>
    <category>
        <pattern>* WHATSUP *</pattern>
        <template>
            My friends call me
            <bot name="name" />
        </template>
    </category>
    <category>
        <pattern>JUCA *</pattern>
        <template>
            JOD,
            <star />
            <star />
        </template>
    </category>
    <category>
        <pattern>(YOU|YOUR)? * NAME *</pattern>
        <template>
            My friends call me
            <bot name="name" />
        </template>
    </category>
    <category>
        <pattern>DO YOU KNOW SOMETHING</pattern>
        <template>
            <learn>example_learn.aiml</learn> Try again
        </template>
    </category>
</aiml>